{
  "name": "er-theme",
  "version": "1.0.0",
  "description": "Er theme with CSS",
  "main": "index.js",
  "scripts": {
    "dev:css": "npx @tailwindcss/cli -i ./assets/css/app.css -o ./assets/css/main.css --watch",
    "dev:server": "hugo server --disableFastRender",
    "build:css": "npx @tailwindcss/cli -i ./assets/css/app.css -o ./assets/css/main.css --minify",
    "build": "npx run build:css && hugo --minify"
  },
  "keywords": [
    "hugo",
    "theme"
  ],
  "author": "Darwin Wu",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.12",
    "@tailwindcss/typography": "^0.5.16",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.12"
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
}
